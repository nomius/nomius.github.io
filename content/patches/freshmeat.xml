<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Freshmeat</ShortName>
<Description>Freshmeat Search</Description>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">data:image/png;base64,%3D</Image>
<Url type="text/html" method="GET" template="http://freshmeat.net/search/">
  <Param name="q" value="{searchTerms}"/>
  <Param name="section" value="projects"/>
</Url>
<SearchForm>http://freshmeat.net</SearchForm>
</SearchPlugin>
